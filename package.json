{
  "name": "dashboard",
  "version": "0.1.0",
  "description": "Web interface for the SmartHut",
  "author": "Leonid Boykov <leonid.v.boykov@gmail.com>",
  "private": true,
  "dependencies": {
    "husky": "^0.14.3",
    "lint-staged": "^7.0.4",
    "prettier": "^1.12.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-router": "^4.2.0",
    "react-scripts": "1.1.4"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": ["prettier --single-quote --write", "git add"]
  },
